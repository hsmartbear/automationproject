	</test>

	<test name="Project TestSuite" enabled="false">
		<classes>
			<class name="com.qmetry.tests.web.sanity.ProjectTestSuite">
				<methods>
					<exclude name="project_create" />
				</methods>
			</class>
		</classes>
	</test>
</suite>